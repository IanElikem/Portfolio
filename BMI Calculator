# # BMI CALCULATOR
name= 'Ian'
print("Hello", name, ", and welcome to the Cactus Jack BMI Calculator")
Weight=float(input("Enter weight in Kg: "))
Height=float(input("Enter height in m: "))
BMI= (Weight)/(Height*2)
print(BMI)


if BMI<18.5:
    print("Underweight")
elif BMI<25:
    print("Normal weight")
elif BMI<30:
    print("Overweight")
else:
    print("Obese")
